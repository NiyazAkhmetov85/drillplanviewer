{
  "functions": {
    "api/**/*.js": {
      "includeFiles": "dist/**"
    }
  },
  "crons": [
    {
      "path": "/api/cron/check",
      "schedule": "0 0 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "speedInsights": {
    "enabled": false
  },
  "analytics": {
    "enabled": false
  }
}
